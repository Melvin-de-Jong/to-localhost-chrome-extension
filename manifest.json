{
    "manifest_version": 2,
    "name": "To Localhost",
    "version": "0.0.1",
    "description": "Converts url to localhost:8888/$1",
    "browser_action": {
        "default_icon": "images/icon128-1.png",
        "default_title": "T Localhost"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
      },
    "permissions": [
        "webNavigation",
        "tabs",
        "storage"
    ]
}